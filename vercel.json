{
  "version": 2,
  "builds": [
    {
      "src": "server-postgres.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-postgres.cjs"
    },
    {
      "src": "/((?!api/).*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/((?!api/).*)",
      "dest": "/dist/index.html",
      "status": 200
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "NODE_ENV": "production"
  }
}

